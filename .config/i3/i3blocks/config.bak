[Disk]
label=
command=~/.config/i3/i3blocks/blocks/disk
color=#F0FFFF
interval=30

[Cpu]
label=
command=~/.config/i3/i3blocks/blocks/cpu
color=#F0FFFF
interval=1

[Date]
label=
command=~/.config/i3/i3blocks/blocks/date
color=#F0FFFF
interval=30

[Time]
#label=T
command=~/.config/i3/i3blocks/blocks/time
color=#F0FFFF
interval=1

#[Bandwidth down]
#label=
#command=~/.config/i3/i3blocks/blocks/bandwidth-down
